<Window x:Class="Starvers.Managing.ManagingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Starvers.Managing"
        mc:Ignorable="d"
        Title="ManagingWindow" Height="550" Width="900"
		ResizeMode="NoResize"
		Loaded="Window_Loaded" Closing="Window_Closing">
	<Window.Background>
		<ImageBrush ImageSource="Background.png" Stretch="UniformToFill"/>
	</Window.Background>
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="WPFResources.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>
	<Grid>
		<Button Style="{StaticResource MBButton}" Click="Button_Click">N</Button>

	</Grid>
</Window>
